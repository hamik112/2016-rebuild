<div id="home-in-line-two" class="home-module-wrapper in-line-features-two-wrapper {{ settings.home-in-line-two-style }}">
  <div class="home-module home-in-line-features">
    {% if settings.home-in-line-two-heading.size > 0 %}
      <h3 class="section-title">{{ settings.home-in-line-two-heading }}<span class="tasticon-headarrow-r"></span></h3>
    {% else %}
      {% unless settings.home-show-in-line-two-1 or settings.home-show-in-line-two-2 or settings.home-show-in-line-two-3 or settings.home-show-in-line-two-4 or settings.home-show-in-line-two-5 or settings.home-show-in-line-two-6 %}
