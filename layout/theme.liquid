  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

  {% if template == 'index' %}
    <title>{{ shop.name }} | {{ page_title }}</title>
  {% elsif template == '404' %}
    <title>Page Not Found | {{ shop.name }}</title>
  {% else %}
    <title>{{ page_title }} | {{ shop.name }}</title>
  {% endif %}

  {% if page_description %}
    <meta name="description" content="{{ page_description | escape }}" />
  {% endif %}
  
  <!-- Icons -->
  <link rel="apple-touch-icon" sizes="57x57" href="{{ 'webapp_57.png' | asset_url }}">
  <link rel="apple-touch-icon" sizes="60x60" href="{{ 'webapp_60.png' | asset_url }}">
  <link rel="apple-touch-icon" sizes="72x72" href="{{ 'webapp_72.png' | asset_url }}">
  <link rel="apple-touch-icon" sizes="76x76" href="{{ 'webapp_76.png' | asset_url }}">
  <link rel="apple-touch-icon" sizes="114x114" href="{{ 'webapp_114.png' | asset_url }}">
  <link rel="apple-touch-icon" sizes="120x120" href="{{ 'webapp_120.png' | asset_url }}">
  <link rel="apple-touch-icon" sizes="144x144" href="{{ 'webapp_144.png' | asset_url }}">
  <link rel="apple-touch-icon" sizes="152x152" href="{{ 'webapp_152.png' | asset_url }}">
  <link rel="apple-touch-icon" sizes="180x180" href="{{ 'webapp_180.png' | asset_url }}">
  <link rel="icon" type="image/png" href="{{ 'favicon_32.png' | asset_url }}" sizes="32x32">
  <link rel="icon" type="image/png" href="{{ 'webapp_192.png' | asset_url }}" sizes="192x192">
  <link rel="icon" type="image/png" href="{{ 'favicon_96.png' | asset_url }}" sizes="96x96">
  <link rel="icon" type="image/png" href="{{ 'favicon_16.png' | asset_url }}" sizes="16x16">
  <link rel="mask-icon" href="{{ 'safari-pinned-tab.svg ' | asset_url }}" color="#82567c">
  <meta name="theme-color" content="#ffffff">

  <link rel="canonical" href="{{ canonical_url }}" />
  <meta name="viewport" content="width=device-width,initial-scale=1" />

  {{ content_for_header }}

  {% include 'social-meta' %}

  {% include 'fonts' %}
  
  <!-- Theme CSS -->
  {{ 'theme.scss.css' | asset_url | stylesheet_tag }}
  
  <!-- Custom CSS -->
  {{ 'jquery.fancybox.css' | asset_url | stylesheet_tag }}
  {{ 'inspira-styles.scss.css' | asset_url | stylesheet_tag }}
  {{ 'inspira-media.scss.css' | asset_url | stylesheet_tag }}

  <!-- Third Party JS Libraries -->
  {{ 'modernizr-2.7.1.min.js' | asset_url | script_tag }}
  
  <!-- Store object -->
  {% include 'theme-options' %}

  <style type="text/css">
    {% assign hex2rgb_hex = settings.primary-button-background %}
    {% include 'hex2rgb' %}
    .add-to-cart-loader, .home-slideshow-loader, .product-slideshow-loader {
      border-left:   3px solid rgba({{ hex2rgb_r }}, {{ hex2rgb_g }}, {{ hex2rgb_b }}, .15);
      border-right:  3px solid rgba({{ hex2rgb_r }}, {{ hex2rgb_g }}, {{ hex2rgb_b }}, .15);
      border-bottom: 3px solid rgba({{ hex2rgb_r }}, {{ hex2rgb_g }}, {{ hex2rgb_b }}, .15);
      border-top:    3px solid rgba({{ hex2rgb_r }}, {{ hex2rgb_g }}, {{ hex2rgb_b }}, .8);
    }
    {% assign hex2rgb_hex = settings.primary-button-color %}
    {% include 'hex2rgb' %}
    .header-drawer h1, .header-drawer .navigation .primary ul {
      border-color: {{settings.primary-button-color}};
      border-color: rgba({{ hex2rgb_r }}, {{ hex2rgb_g }}, {{ hex2rgb_b }}, .3);
    }
    {% assign hex2rgb_hex = settings.footer-accent-color %}
    {% include 'hex2rgb' %}
    .copyright, .payment-options {
      color: rgba({{ hex2rgb_r }}, {{ hex2rgb_g }}, {{ hex2rgb_b }}, .5);
    }
    .spread-the-word .footer-share-icon {
      background: rgba({{ hex2rgb_r }}, {{ hex2rgb_g }}, {{ hex2rgb_b }}, .7);
    }
    .spread-the-word .footer-share-icon:hover {
      background: rgba({{ hex2rgb_r }}, {{ hex2rgb_g }}, {{ hex2rgb_b }}, .9);
    }

    {% if settings.custom-css != blank %}
      {{ settings.custom-css }}
    {% endif %}
  </style>
  
